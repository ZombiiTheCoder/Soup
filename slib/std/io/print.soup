fn GoFmtPrint(Val){
    if (Type(Val) != "String"){
        Soup.exit(1)
    }
    go.run(`
        import "fmt"
        fmt.Print("`+Val+`")
    `)
}

fn GoFmtPrintLn(Val){
    if (Type(Val) != "String"){
        Soup.exit(1)
    }
    go.run(`
        import "fmt"
        fmt.Println("`+Val+`")
    `)
}